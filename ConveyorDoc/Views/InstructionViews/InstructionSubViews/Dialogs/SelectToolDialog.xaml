<UserControl
    x:Class="ConveyorDoc.Views.InstructionViews.InstructionSubViews.Dialogs.SelectToolDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:reusable="clr-namespace:ConveyorDoc.Views.Reusable"
    xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
    xmlns:local="clr-namespace:ConveyorDoc.Views.InstructionViews.InstructionSubViews.Dialogs"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    d:DesignHeight="450"
    d:DesignWidth="800"
    prism:ViewModelLocator.AutoWireViewModel="True"
    mc:Ignorable="d"
    x:Name="_AddToolDialog">

    <reusable:ToolDataGrid DataContext="{Binding ElementName=_AddToolDialog, Path=DataContext}"/>

</UserControl>
