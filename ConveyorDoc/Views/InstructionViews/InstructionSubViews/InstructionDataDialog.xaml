<UserControl
    x:Class="ConveyorDoc.Views.InstructionViews.InstructionSubViews.InstructionDataDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ConveyorDoc.Views.InstructionViews.InstructionSubViews"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:reusable="clr-namespace:ConveyorDoc.Views.Reusable"
    Width="300"
    prism:ViewModelLocator.AutoWireViewModel="True"
    Background="Transparent"
    BorderThickness="2"
    mc:Ignorable="d"
    x:Name="_InstructionDataDialog">
    <reusable:InstructionContent DataContext="{Binding ElementName=_InstructionDataDialog, Path=DataContext}" />
</UserControl>
